<!DOCTYPE html>
<html>
<head>
  <title>YouPick</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag "application", 'http://fonts.googleapis.com/css?family=Varela Round', :media => "all" %>
  <%= stylesheet_link_tag "application", 'http://fonts.googleapis.com/css?family=Dosis', :media => "all" %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if user_signed_in? %>
  <%= render "layouts/sidebar" %>
	<div id="mainspace">
		<%= yield %>
	</div>
<% else %>
	<%= yield %>
<% end %>


</body>
</html>
