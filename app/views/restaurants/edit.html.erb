<h1>Restaurants#edit</h1>
<%= form_for(@restaurant) do |f| %> 
    <div class="field">
        <p>&nbsp;&nbsp;name</p> 
        &nbsp;&nbsp;<%= f.text_field :name%>
    </div>
    <div class="field">
        <p>&nbsp;&nbsp;link</p>
        &nbsp;&nbsp;<%= f.text_field :link%>
    </div>
    <div class="actions">
        &nbsp;&nbsp;<%=f.submit%>
    </div>
<% end %>

<%= link_to "Back to Main", root_path%>
